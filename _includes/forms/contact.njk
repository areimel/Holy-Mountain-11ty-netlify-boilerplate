<form class="--hm_form_1" id="contact_form"  method="post" autocomplete="on" data-netlify="true" netlify-honeypot="bot-field" data-netlify-recaptcha="true">

	<!-- META -->
	<input type="hidden" name="data[form]" value="Contact Form" />
	<p class="hidden">
	    <label>Donâ€™t fill this out if you're human: <input name="bot-field" type="hidden" /></label>
	  </p>

	<!-- FIRST NAME -->
	<label class="input_wrapper fancy_label half">
		<input type="text" name="data[f_name]" placeholder="" required aria-label="Name">
		<span class="visual_label">First Name*</span>
	</label>	
	
	<!-- LAST NAME -->
	<label class="input_wrapper fancy_label half">
		<input type="text" name="data[l_name]" placeholder="" required aria-label="Name">
		<span class="visual_label">Last  Name*</span>
	</label>
	
	<!-- EMAIL -->
	<label class="input_wrapper fancy_label half">
		<input type="email" name="data[email]" placeholder="" required aria-label="Email">
		<span class="visual_label">Email*</span>
	</label>
	
	<!-- PHONE -->
	<label class="input_wrapper fancy_label half">
		<input type="text" name="data[phone]" placeholder="" required aria-label="Phone">
		<span class="visual_label">Phone*</span>
	</label>
	
	<!-- COMMENTS -->
	<label class="input_wrapper fancy_label full">
		<textarea name="data[comments]" placeholder="" required aria-label="Comments"></textarea>
		<span class="visual_label">Comments*</span>
	</label>

	<div class="submit_wrapper">
		<!-- NETLIFY RECAPTCHA -->
		<div data-netlify-recaptcha="true"></div>
		
		<input class="" type="submit" name="" value="SUBMIT" aria-label="Submit">
		<!--<button class="submit">SUBMIT</button>-->
	</div>
</form>